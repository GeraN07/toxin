{
    "version": 2,
    "builds": [
      {
        "src": "api/rooms.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/rooms",
        "dest": "/api/rooms.js"
      },
      {
        "src": "/api/rooms/:id",
        "dest": "/api/rooms.js?id=:id"
      }
    ]
  }
  