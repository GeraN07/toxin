{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/rooms",
        "dest": "/api/rooms.js"
      },
      {
        "src": "/api/rooms/:id",
        "dest": "/api/rooms.js?id=:id"
      }
    ],
    "build": {
      "env": {
        "NPM_FLAGS": "--legacy-peer-deps"
      }
    }
  }
  